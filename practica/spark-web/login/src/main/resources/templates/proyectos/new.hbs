{{#partial "contenido"}}
	
	<form action="/proyectos" method="POST">
		
		<div class="form-group">
			<label for="nombre">Nombre</label>
			<input type="text" placeholder="Nombre del proyecto" name="nombre">
		</div>
	
		<div class="form-group">		
			<label for="costoEstimado">Costo Estimado</label>
			<input type="number" placeholder="Costo estimado del proyecto" name="costoEstimado">
		</div>
		
		<input class="btn btn-primary btn-lg" type="submit" value="Guardar">
	</form>
{{/partial}}

{{>layout.hbs}}