{{#partial "contenido"}}
	
	<form action="/consultoras" method="POST">
		
		<div class="form-group">
			<label for="nombre">Nombre</label>
			<input type="text" placeholder="Nombre de la Consultora" name="nombre">
		</div>
	
		<div class="form-group">		
			<label for="cantidadEmpleados">Cantidad Empleados</label>
			<input type="number" placeholder="Cantidad Empleados" name="cantidadEmpleados">
		</div>
		
