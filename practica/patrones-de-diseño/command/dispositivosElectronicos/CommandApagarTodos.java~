/**
 * Concrete Command - Apagar todos los dispositivos
 */

public class CommandApagarTodos implements Command{
  DispositivoElectronico dispositivo;
  
  public CommandApagarTodos(DispositivoElectronico dispositivo){
    this.dispositivo = dispositivo;
  }
  
  public void ejecutar(){
    dispositivo.accionEncender();
  }
}
